{
  "name": "react-redux",
  "title": "React Redux",
  "description": "Starter Kit for React with redux and router",
  "version": "0.1.0",
  "author": "Gil Barbara <gilbarbara@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/gilbarbara/react-redux"
  },
  "bugs": "https://github.com/gilbarbara/react-redux/issues",
  "license": "MIT",
  "dependencies": {
    "classlist-polyfill": "^1.0",
    "classnames": "^2.2",
    "core-decorators": "^0.9",
    "core-js": "^2.0",
    "fbjs": "^0.6",
    "mobile-detect": "^1.3",
    "modernizr": "^3.2",
    "react": "^0.14",
    "react-addons-update": "^0.14",
    "react-dom": "^0.14",
    "react-redux": "^4.0",
    "react-router": "^2.0.0-rc4",
    "redux": "^3.0",
    "redux-logger": "^2.3",
    "redux-simple-router": "^1.0",
    "whatwg-fetch": "^0.10"
  },
  "devDependencies": {
    "apache-server-configs": "^2.14",
    "babel-core": "^6.3",
    "babel-eslint": "^4.1",
    "babel-plugin-add-module-exports": "^0.1",
    "babel-plugin-react-transform": "^2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3",
    "babel-preset-es2015": "^6.3",
    "babel-preset-react": "^6.3",
    "babel-preset-stage-1": "^6.3",
    "babel-register": "^6.3",
    "babelify": "^7.2",
    "browser-sync": "^2.10",
    "browserify": "^12.0",
    "browserify-shim": "^3.8",
    "connect-history-api-fallback": "1.1",
    "debowerify": "^1.3",
    "deep-freeze": "0.0.1",
    "del": "^2.2",
    "envify": "^3.4",
    "eslint": "^1.10",
    "eslint-config-airbnb": "^2.1",
    "eslint-plugin-jsdoc": "^2.2",
    "eslint-plugin-react": "^3.13",
    "eslint-plugin-require-jsdoc": "^1.0",
    "expect": "^1.13",
    "gulp": "^3.9",
    "gulp-autoprefixer": "^3.1",
    "gulp-cache": "^0.4",
    "gulp-changed": "^1.3",
    "gulp-cssmin": "^0.1",
    "gulp-eslint": "^1.1",
    "gulp-filelog": "^0.4",
    "gulp-filter": "^3.0",
    "gulp-flatten": "^0.2",
    "gulp-gh-pages": "^0.5",
    "gulp-if": "^2.0",
    "gulp-imagemin": "^2.4",
    "gulp-load-plugins": "^1.1",
    "gulp-mocha": "^2.2",
    "gulp-plumber": "^1.0",
    "gulp-replace": "^0.5",
    "gulp-sass": "^2.1",
    "gulp-size": "^2.0",
    "gulp-streamify": "1.0",
    "gulp-tap": "^0.1",
    "gulp-uglify": "^1.5",
    "gulp-useref": "^3.0",
    "gulp-util": "^3.0",
    "livereactload": "^2.1",
    "merge-stream": "^1.0",
    "minami": "^1.1",
    "nock": "^3.6",
    "react-proxy": "^1.1",
    "redux-api-middleware": "^1.0.0-beta3",
    "redux-devtools": "^3.0",
    "redux-devtools-dock-monitor": "^1.0",
    "redux-devtools-log-monitor": "^1.0",
    "redux-mock-store": "0.0.4",
    "redux-thunk": "^1.0",
    "run-sequence": "^1.1",
    "sandboxed-module": "^2.0",
    "vinyl-buffer": "^1.0",
    "vinyl-paths": "^2.1",
    "vinyl-source-stream": "^1.1",
    "watchify": "^3.6"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-1"
    ],
    "plugins": [
      "add-module-exports",
      "transform-decorators-legacy"
    ],
    "env": {
      "development": {
        "plugins": [
          [
            "react-transform",
            {
              "transforms": [
                {
                  "transform": "livereactload/babel-transform",
                  "imports": [
                    "react"
                  ]
                }
              ]
            }
          ]
        ]
      }
    }
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "ignore": "/bower_components/",
          "sourceMapRelative": "."
        }
      ],
      "browserify-shim",
      "debowerify",
      [
        "envify",
        {
          "_": "purge"
        }
      ]
    ]
  },
  "browser": {
    "jquery": "./bower_components/jquery/dist/jquery.js"
  },
  "browserify-shim": {
    "jquery": "$"
  },
  "scripts": {
    "start": "gulp serve",
    "lint": "gulp lint",
    "build": "gulp build",
    "test": "gulp mocha",
    "test-watch": "export NODE_ENV=test && ./node_modules/.bin/_mocha --watch --compilers js:babel-register $(find app/scripts/__tests__ -name *.spec.js)"
  },
  "engines": {
    "node": ">=0.12.0"
  }
}
